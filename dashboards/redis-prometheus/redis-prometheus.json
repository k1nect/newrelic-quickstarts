{
    "name": "Redis-Prometheus",
    "description": "Dashboard for displaying Redis metrics from Prometheus",
    "pages": [
      {
        "name": "Redis",
        "description": null,
        "widgets": [
          {
            "title": "Reporting Nodes",
            "layout": {
              "column": 1,
              "row": 1,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.billboard"
            },
            "rawConfiguration": {
              "dataFormatters": [],
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT  filter(uniqueCount(role), WHERE role  = 'slave') as 'Slaves', filter(uniqueCount(role), WHERE role  = 'master') as 'Master' FROM Metric where metricName = 'redis_instance_info' until 1 minute ago"
                }
              ],
              "thresholds": []
            }
          },
          {
            "title": "Commands Processed",
            "layout": {
              "column": 5,
              "row": 1,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT sum(redis_commands_processed_total)  as 'Commands' FROM Metric  where metricName = 'redis_commands_processed_total'  TIMESERIES until 1 minutes ago"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "",
            "layout": {
              "column": 9,
              "row": 1,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.markdown"
            },
            "rawConfiguration": {
              "text": "\n![Redis Logo](https://redis.com/wp-content/uploads/2021/08/redis-logo.png)\n\n\nRedis is an open source, in-memory data structure store, used as a database, cache and message broker. It supports data structures such as strings, hashes, lists, sets, sorted sets with range queries, bitmaps, hyperloglogs and spatial indexes.\n\n\n"
            }
          },
          {
            "title": "Commands by node",
            "layout": {
              "column": 1,
              "row": 4,
              "width": 12,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT sum(redis_commands_processed_total) as 'Commands' FROM Metric TIMESERIES FACET instance"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "Connected Clients",
            "layout": {
              "column": 1,
              "row": 7,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT sum(redis_connected_clients)  as 'Connected clients' FROM Metric where metricName = 'redis_connected_clients' TIMESERIES until 1 minutes ago"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "Connected Clients, by node",
            "layout": {
              "column": 5,
              "row": 7,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT sum(redis_connected_clients)  as 'Connected clients' FROM Metric where metricName = 'redis_connected_clients' Facet instance TIMESERIES until 1 minutes ago"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "Changes Since Last Save, by node",
            "layout": {
              "column": 9,
              "row": 7,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT average(redis_rdb_changes_since_last_save) as 'Rdb changes' FROM Metric TIMESERIES FACET instance"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "Blocked Clients",
            "layout": {
              "column": 1,
              "row": 10,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT average(redis_blocked_clients) * uniqueCount(instance) AS 'Blocked Clients' FROM Metric  TIMESERIES until 1 minutes ago"
                }
              ],
              "yAxisLeft": {
                "zero": true
              }
            }
          },
          {
            "title": "Redis Used Memory (bytes), by node",
            "layout": {
              "column": 5,
              "row": 10,
              "width": 4,
              "height": 3
            },
            "linkedEntityGuids": null,
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
              },
              "nrqlQueries": [
                {
                  "accountId" : 0,
                  "query": "SELECT average(redis_memory_used_bytes) as 'Used memory' FROM Metric TIMESERIES FACET instance "
                }
